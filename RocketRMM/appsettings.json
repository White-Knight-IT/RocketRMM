{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "ApiSettings": {
    "DataPath": "/root/rocketrmm/persistent/data",
    "CachePath": "/root/rocketrmm/cache",
    "PersistentPath": "/root/rocketrmm/persistent",
    "WebRootPath": "/root/rocketrmm/persistent/www/rocketrmm-webfront",
    "ShowSwaggerUi": true,
    "RunSwagger": true,
    "ServeStaticFiles": false,
    "ShowDevEndpoints": false,
    "WebUiUrl": "https://rocketrmm.yourdomain.com",
    "DbSettings": {
      "Db": "rocketrmmdb",
      "DbUser": "rocketrmmcoreservice",
      "DbPassword": "wellknownpassword",
      "DbServer": "localhost",
      "DbServerPort": "21124"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:8088"
      }
    }
  },
  "AllowedHosts": "*"
}
